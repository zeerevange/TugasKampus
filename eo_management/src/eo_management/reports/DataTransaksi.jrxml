	<subDataset name="New Data Transaksi1" uuid="9b65138f-bc1e-4a2b-b988-67a00100218b">
		<queryString language="SQL">
			<![CDATA[SELECT 
    `pesanan_layanan`.`id`,
    `pesanan_layanan`.`paket_layanan_id`,
    `pesanan_layanan`.`jumlah_peserta`,
    `pesanan_layanan`.`status_lunas`,
    `paket_layanan`.`nama` AS `nama_paket`,
    `paket_layanan`.`harga`,
    `pelanggan`.`nama` AS `nama_pic`,
    `pelanggan`.`no_telp`,
    `perusahaan_pelanggan`.`nama_perusahaan`,
    (`paket_layanan`.`harga` * `pesanan_layanan`.`jumlah_peserta`) AS `total_harga`
FROM 
    `pesanan_layanan`
LEFT JOIN 
    `paket_layanan` ON `paket_layanan`.`id` = `pesanan_layanan`.`paket_layanan_id`
LEFT JOIN 
    `pelanggan` ON `pelanggan`.`id` = `pesanan_layanan`.`pelanggan_id`
LEFT JOIN 
    `perusahaan_pelanggan` ON `perusahaan_pelanggan`.`pelanggan_id` = `pelanggan`.`id`;]]>
		</queryString>
