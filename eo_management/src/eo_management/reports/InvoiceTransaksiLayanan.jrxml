	<parameter name="NO_INVOICE" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT 
`pesanan_layanan`.`id`,
`pelanggan`.`nama` AS `nama_pic`,
`perusahaan_pelanggan`.`nama_perusahaan` AS `bill_to`,
`perusahaan_pelanggan`.`alamat_perusahaan` AS `alamat_bill_to`,
`pelanggan`.`no_telp`,
`pelanggan`.`email`,
`paket_layanan`.`nama` AS `nama_paket_layanan`,
`paket_layanan`.`harga`,
`paket_layanan`.`include`,
`paket_layanan`.`deskripsi`,
`pesanan_layanan`.`jumlah_peserta`,
(`paket_layanan`.`harga` * `pesanan_layanan`.`jumlah_peserta`) AS `total_harga`,
`pesanan_layanan`.`status_lunas`
FROM `pesanan_layanan`
LEFT JOIN `paket_layanan` ON `paket_layanan`.`id` = `pesanan_layanan`.`paket_layanan_id`
LEFT JOIN `pelanggan` ON `pelanggan`.`id` = `pesanan_layanan`.`pelanggan_id`
LEFT JOIN `perusahaan_pelanggan` ON `perusahaan_pelanggan`.`pelanggan_id` = `pelanggan`.`id`
WHERE `pesanan_layanan`.`id` = $P{NO_INVOICE};]]>
	</queryString>
